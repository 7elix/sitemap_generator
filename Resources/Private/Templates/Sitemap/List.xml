<html xmlns:f="http://typo3.org/ns/TYPO3/CMS/Fluid/ViewHelpers" data-namespace-typo3-fluid="true">
  <f:layout name="Default"/>
<f:section name="main"><urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <f:for each="{urlEntries}" as="urlEntry"><url>
    <loc>
      {urlEntry.loc -> f:format.htmlentities()}
    </loc>
    <f:if condition="{urlEntry.lastmod}"><lastmod>
      {urlEntry.lastmod}
    </lastmod></f:if>
    <priority>
      {urlEntry.priority}
    </priority>
    <f:if condition="{urlEntry.changefreq}"><changefreq>
      {urlEntry.changefreq}
    </changefreq></f:if>
  </url>
  </f:for>
</urlset>
  </f:section>
</html>